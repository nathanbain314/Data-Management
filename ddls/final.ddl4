CREATE OR REPLACE TRIGGER emp_date_trigger
BEFORE INSERT OR UPDATE OF EMPLOYEE_STATUS
ON NL_ARL_EMP
FOR EACH ROW
BEGIN
	:NEW.status_effective_date := SYSDATE;
END;
/

CREATE OR REPLACE TRIGGER status_date_trigger
BEFORE INSERT OR UPDATE OF STATUS
ON NL_N_LIST
FOR EACH ROW
BEGIN
	:NEW.status_effective_date := SYSDATE;
END;
/

CREATE OR REPLACE TRIGGER Creation_date_trigger
BEFORE INSERT
ON NL_N_LIST
FOR EACH ROW
BEGIN
	:NEW.CREATION_DATE := SYSDATE;
END;
/
